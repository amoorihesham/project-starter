{
  "display": "Node LTS (22)",
  "_version": "22.0.0",
  "compilerOptions": {
    "lib": ["ES2015"],
    "module": "node16",
    "target": "es2022",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "outDir": "dist"
  },
  "include": ["src"]
}
